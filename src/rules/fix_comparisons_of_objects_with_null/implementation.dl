/* Pre  [:x].equals(null) */
/* Post [:x] == null      */
rewrite("fix_comparisons_of_objects_with_null", filename, start, end, cat(subject_str, " == null")) :-
    method_invocation(id, subject, "equals", [arg, nil]),
        filename_of(id, filename),
        starts_at(id, start),
        ends_at(id, end),
    string_representation(subject, subject_str),
    null_literal(arg).

