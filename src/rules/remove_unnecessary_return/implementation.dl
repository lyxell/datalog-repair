/* Pre  return; */
/* Post         */
rewrite("remove_unnecessary_return", filename, start, end, "") :-
    method_declaration(_, _, _, body),
    block(body, stmts),
    list_last_element(stmts, return),
    return_statement(return, nil),
    starts_at(return, start),
    ends_at(return, end),
    filename_of(return, filename).
