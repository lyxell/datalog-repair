rewrite("S2204", filename, start, end, cat(left_str, ".get() == ", right_str, ".get()")) :-
    method_invocation(id, left, "equals", [right, nil]),
        filename_of(id, filename),
        starts_at(id, start),
        ends_at(id, end),
    has_atomic_class_type(left),
    has_atomic_class_type(right),
    string_representation(left, left_str),
    string_representation(right, right_str).
