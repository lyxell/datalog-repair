rewrite("remove_redundant_casts", filename, start, end, value_str) :-
    cast_expression(id, cast_type, value),
        starts_at(id, start),
        ends_at(id, end),
        filename_of(id, filename),
    has_type(value, actual_type),
    string_representation(value, value_str),
    string_representation(cast_type, type_str),
    string_representation(actual_type, type_str).
